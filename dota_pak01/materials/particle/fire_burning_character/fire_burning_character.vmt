"SpriteCard"
{
	$basetexture "particle/fire_burning_character/fire_burning_character"
	$ADDSELF 1.0
	$BLENDFRAMES 1

	$OVERBRIGHTFACTOR 4.5
	//$OVERBRIGHTFACTOR 1.75
	//$MINSIZE .0125
	//$MAXSIZE .6
	$STARTFADESIZE .45
	$ENDFADESIZE .575
	$DEPTHBLEND 1
	$DEPTHBLENDSCALE "20"
	360?$STARTFADESIZE .35
	360?$ENDFADESIZE .45
	360?$BLENDFRAMES 0
	"GPU<2?$STARTFADESIZE" ".35"
	"GPU<2?$ENDFADESIZE" ".45"
	"GPU<2?$BLENDFRAMES" "0"
}